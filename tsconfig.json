{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "es2015"],
    "jsx": "react",
    "module": "es2015",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["client/src/**/*"],
  "exclude": ["node_modules"]
}